{
    "name": "eghojansu/stick",
    "description": "Stick~to~PHP Framework",
    "license": "MIT",
    "type": "library",
    "author": [{
        "name": "Eko Kurniawan",
        "email": "ekokurniawanbs@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "Fal\\Stick\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fal\\Stick\\Test\\": "tests/src/",
            "Fixture\\": "tests/fixture/classes/"
        }
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "ext-gd": "*",
        "ext-mysql": "*",
        "ext-pdo": "*",
        "ext-sqlite3": "*",
        "ext-zip": "*",
        "phpunit/phpunit": "^7.4"
    },
    "suggest": {
        "ext-gd": "To use image manipulation helper",
        "ext-pdo": "To use sql mapper",
        "ext-zip": "To use zip helper",
        "phpunit/phpunit": "To use functional-test response helper"
    },
    "bin": [
        "stick"
    ]
}
